# Local environment overrides for sqs_handler
# Replace the placeholders with your real values. Do NOT commit secrets to source control.

# AWS credentials (optional if using IAM role or shared credentials)
AWS_ACCESS_KEY_ID = ""
AWS_SECRET_ACCESS_KEY = ""
# AWS_SESSION_TOKEN=

# AWS region
AWS_REGION=us-east-1

# SQS FIFO queue URL and DLQ URL
SQS_QUEUE_URL=
DLQ_URL=

# Local module path (defaults to ./src/modular_waste_predictor)
LOCAL_MODULE_PATH=

# Polling options
POLL_WAIT=20
MAX_MESSAGES=10

MONGODB_URL=
